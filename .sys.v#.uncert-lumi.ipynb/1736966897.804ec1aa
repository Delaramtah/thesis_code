    "def AccError(N_pass, N_tot, ifRel):\n",
    "    Acc = float(N_pass)/float(N_tot)\n",
    "    return Acc, RatioError(N_pass, N_tot, math.sqrt(N_pass), math.sqrt(N_tot), ifRel)\n"
   ]
